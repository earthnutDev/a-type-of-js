{
  "name": "a-type-of-js",
  "type": "module",
  "version": "0.1.5",
  "main": "cjs/index.cjs",
  "module": "mjs/index.mjs",
  "types": "types/index.d.ts",
  "author": {
    "name": "earthnut",
    "email": "earthnut.dev@outlook.com",
    "url": "https://earthnut.dev"
  },
  "description": "type of function for js/ts",
  "scripts": {
    "b": "rollup --config rollup.config.js && tsc -p tsconfig.types.json",
    "clean:package": "node scripts/clean-package-json.js",
    "build": "npx ixxx rm dist && npm run b && npm run clean:package",
    "push:version": "npm run beautify && bash -c 'git add . && git commit -m \"version: $1 $(date +\"%Y-%m-%d %H:%M:%S\")${2:+ }$2\" && git tag -a \"v$1\" -m \"$(date +\"%Y-%m-%d %H:%M:%S\")${2:+ }$2\" && git push origin main --tags' -- ",
    "push:submit": "npm run beautify && bash -c 'git add . && git commit -m \"submit: $(date +\"%Y-%m-%d %H:%M:%S\")${1:+ }$1\" && git push origin main' -- ",
    "clean": "npx ixxx up -d r node_modules package-lock.json && npm install && npm run build",
    "diff": "npx ixxx pkg diff",
    "eslint": "npx ixxx cls && eslint src",
    "beautify": "npm run prettier",
    "prettier": "npx ixxx cls && prettier . --write",
    "test": "npx ixxx clear && npx jest --coverage",
    "report": "cat ./coverage/lcov.info | coveralls",
    "coverage": "codecov",
    "versionUpdate": "npx ixxx up -n",
    "pub": "npm run build && cd dist && npm publish --access public && cd ..",
    "prepare": "husky"
  },
  "files": [
    "cjs",
    "mjs",
    "types",
    "CHANGELOG-en.md"
  ],
  "exports": {
    ".": {
      "import": {
        "default": "./mjs/index.mjs",
        "types": "./types/index.d.ts"
      },
      "require": {
        "default": "./cjs/index.cjs",
        "types": "./types/index.d.ts"
      }
    }
  },
  "keywords": [
    "a-type-of-js"
  ],
  "license": "ISC",
  "homepage": "https://earthnut.dev/a-type-of-js",
  "bugs": {
    "url": "https://github.com/earthnutDev/a-type-of-js/issues",
    "email": "earthnut.dev@outlook.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/earthnutDev/a-type-of-js.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,jex,md,mdx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.25",
    "a-node-tools": "^0.0.15",
    "codecov": "^3.8.3",
    "coveralls": "^3.1.1",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jsdoc": "^50.6.8",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "rollup": "^4.36.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.5.0",
    "ts-jest": "^29.2.6",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0"
  }
}
